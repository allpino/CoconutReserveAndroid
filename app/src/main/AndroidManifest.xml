<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="coco.coconutreserve">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Cinema.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Hotel.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Transportion.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Cinema.CinemaDashboard"
            android:label="@string/title_activity_dashboard" />
        <activity
            android:name=".Hotel.HotelDashboard"
            android:label="@string/title_activity_dashboard" />

        <activity
            android:name=".Transportion.TransportionDashboard"
            android:label="@string/title_activity_dashboard" />

        <activity
            android:name=".Home"
            android:label="@string/title_activity_home" />
        <activity
            android:name=".Notifications"
            android:label="@string/title_activity_notifications" />
        <activity android:name=".Cinema.SaloonViewAsList" />
        <activity android:name=".Hotel.HotelViewAsList" />
        <activity android:name=".Transportion.TransportionViewAsList" />
        <activity android:name=".Payment" />
        <activity android:name=".PaymentSuccess" />
        <activity
            android:name=".AccountSettings"
            android:label="@string/title_activity_account_settings" />
        <activity android:name=".SeatSelection" />
        <activity android:name=".WalletInfo" />
        <activity android:name=".WalletPayment"></activity>
    </application>

</manifest>